<div class="card mb-2">
  <div class="card-header bg-dark text-white">
    <h5><i class="far fa-chart-bar"></i> Stats</h5>
  </div>
  <div class="card-body">
    <p><i class="fas fa-images"></i> Images: <%=stats.images %></p>
    <p>
      <i class="fas fa-comments"></i> Comments:<%=stats.comments %>
    </p>
    <p><i class="fas fa-eye"></i> Views:<%=stats.views %></p>
    <p><i class="fas fa-heart"></i> Likes:<%=stats.likes%> </p>
  </div>
</div>
